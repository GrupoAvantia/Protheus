FmtLin({Padr(RetTitle("A2_CGC"),9),SM0->M0_CGC,If( cPaisLoc$"ARG|POR|EUA", Space(20) , STR0022 + ": " +SM0->M0_INSC),AllTrim(SA2->A2_TEL),SA2->A2_FAX},aLay[07],,,@Li)
