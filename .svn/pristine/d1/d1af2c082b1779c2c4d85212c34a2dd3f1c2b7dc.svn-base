          FmtLin(@aDados,aL[19],cPictVl,,@nLin)
       ELSE 
          IF nTipoMov==1 .AND. MV_PAR25 == 1
             aDados[5]		:=STR0034
             aDados[8]		:=	 nTotvlrctb //TOTAL Valor Contabil
             aDados[13]	:=	 nTotvlrbsc //TOTAL Base de Calculo
         	 If mv_par26 == 1
