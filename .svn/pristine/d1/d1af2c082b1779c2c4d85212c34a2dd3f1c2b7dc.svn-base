			 FmtLin(@aDados,aL[19],,,@nLin)
			 aDados[9]:="======================================"
			 FmtLin(@aDados,aL[19],,,@nLin)
		  Endif
		  If lFirstNC.and.aResumo[i,1]=="NC"
