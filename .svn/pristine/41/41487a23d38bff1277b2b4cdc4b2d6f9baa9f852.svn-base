Local cSelectD1 := ''
Local cWhereD1  := ''
Local cSelectD2 := ''
Local cWhereD2  := ''
Local cSelectD3 := ''
Local cWhereD3  := ''
Local cProdAnt  := ''
Local cLocalAnt := ''
Local cIndice	:= ''
Local cCampo1   := ''
Local cCampo2   := ''
Local cCampo3   := ''
Local cCampo4   := ''
Local cNumSeqTr := '' 
Local cAlias    := ''
Local cWhereB1A := ' ' 
Local cWhereB1B := ' ' 
Local cWhereB1C := ' ' 
Local cWhereB1D := ' ' 

Local cQueryB1A := ' ' 
Local cQueryB1B := ' ' 
Local cQueryB1C := ' ' 
Local cQueryB1D := ' ' 

Local cUnion    := '%%'

Local aDadosTran:= {}

Local lFirst    := .T. 
Local lTransEnd := .T.
Local lContinua := .F.
Local lImpSMov  := .F.
Local lImpS3    := .F.

Local aSalAtu   := { 0,0,0,0,0,0,0 }
