lRet := .t.

nPCustD  := ASCAN(aHeader,{|x|x[2] = "AF3_CUSTD"})
nPQuant  := ASCAN(aHeader,{|x|x[2] = "AF3_QUANT"})
nPQtdOri := ASCAN(aHeader,{|x|x[2] = "AF3_QTDORI"})
