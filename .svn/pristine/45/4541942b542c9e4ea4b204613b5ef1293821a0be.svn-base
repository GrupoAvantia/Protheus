nTCusUni := 0
nTCusVen := 0
nTTotUni := 0
nTTotVen := 0

For _nx:=1 To Len(aCols)
	If _nx <> n
		If aCols[_nx,nPComp]==cComp 
			aCols[_nx,nPIndice] := nIndice
