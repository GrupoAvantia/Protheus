Endif

SetDefault(aReturn,cString)

If nLastKey = 27
	dbClearFilter()
	Return .T.
Endif

RptStatus({|lEnd| C242Imp(aOrd,@lEnd,WnRel,Titulo,Tamanho)},Titulo)

Return Nil

