	AADD(aPosCC,{qSQL->COD_CC,;
	qSQL->DESC_CC,;
	qSQL->COD_NAT,;
	qSQL->DESC_NAT,;
	qSQL->DOCUMENTO,;
	qSQL->TIPO,;
	qSQL->VENCIMENTO,;
	qSQL->CLIFOR,;
	qSQL->NOME,;
	qSQL->VALOR,;
	cTipo,;
	qSQL->COD_CC+qSQL->COD_NAT+qSQL->VENCIMENTO,;
	qSQL->BAIXA,;
	qSQL->E2_PREFIXO})
		
	//Verificar se existe Baixa no SE5 - caso exista lancar no vetor acima.
	nPosAtual:=Len(aPosCC)
	aArea:=GetArea()
	cQuery2:="SELECT "
	cQuery2+="       E5_FILIAL,"
	cQuery2+="       E5_TIPO         ,"
	cQuery2+="       E5_NUMERO , "
	cQuery2+="       E5_PREFIXO   ,"
	cQuery2+="       E5_VALOR         , "
	cQuery2+="       E5_DATA          ,    " 
	cQuery2+="       E5_PARCELA,   "      
	cQuery2+="       E5_TIPODOC   "
	cQuery2+="FROM "
	cQuery2+="	   " + RetSqlName("SE5") + " SE5 "
	cQuery2+="WHERE "
	cQuery2+="	    E5_FILIAL  BETWEEN '"+mv_par01+"' AND '"+mv_par02+"' AND "
	cQuery2+="	    E5_TIPO    ='"+aPosCC[nPosAtual][6]+"' AND "
	cQuery2+="	    E5_NUMERO  ='"+aPosCC[nPosAtual][5]+"' AND "
	cQuery2+="		E5_PREFIXO ='"+aPosCC[nPosAtual][14]+"' AND   "    
	cQuery2+="		E5_CLIFOR  ='"+aPosCC[nPosAtual][08]+"' AND   " 		
	cQuery2+="	    E5_RECPAG =  'P' AND "
	cQuery2+="	    E5_SITUACA <>  'C' AND "
	cQuery2+="		D_E_L_E_T_ <> '*' AND "	
	cQuery2+="	    E5_FILIAL+E5_TIPO+E5_NUMERO+E5_PREFIXO+CAST(E5_VALOR AS VARCHAR)+E5_DATA+E5_PARCELA NOT IN (
	cQuery2+="      SELECT "
	cQuery2+="      E5_FILIAL+   "
	cQuery2+="      E5_TIPO+     "
	cQuery2+="      E5_NUMERO+   "
	cQuery2+="      E5_PREFIXO+  "
	cQuery2+="      CAST(E5_VALOR AS VARCHAR)+      "
	cQuery2+="      E5_DATA+              " 
	cQuery2+="      E5_PARCELA    "
	cQuery2+="FROM "
	cQuery2+="	   " + RetSqlName("SE5") + " SE5 "
	cQuery2+="WHERE "
	cQuery2+="	    E5_FILIAL  BETWEEN '"+mv_par01+"' AND '"+mv_par02+"' AND "
	cQuery2+="	    E5_TIPO    ='"+aPosCC[nPosAtual][6]+"' AND "
	cQuery2+="	    E5_NUMERO  ='"+aPosCC[nPosAtual][5]+"' AND "
	cQuery2+="		E5_PREFIXO ='"+aPosCC[nPosAtual][14]+"' AND   " 
	cQuery2+="	    E5_RECPAG =  'R' AND "
	cQuery2+="		D_E_L_E_T_ <> '*') "	       
	cQuery2+="ORDER BY   "
	cQuery2+="         E5_DATA,  "
	cQuery2+="         E5_NUMERO    "
	cQuery2 := ChangeQuery(cQuery2)
	dbUseArea(.T., "TOPCONN", TCGenQry(,,cQuery2),'qSE5', .F., .T.)
    nSaldo:=0
    nAcrescimos:=0
	nVlrImpostos:=0
    nValor:= 0
    lRateio := .F.
	While !Eof()    
      If qSE5->E5_TIPODOC = 'VL' 
            //Verificar se existe rateio para essa baixa
            nValor:=U_NVerRateio()
            If nValor = 0 
               nValor :=qSE5->E5_VALOR 
            Else
               lRateio:=.T.   
            Endif             
			AADD(aPosCC,{qSQL->COD_CC,;
			qSQL->DESC_CC,;
			qSQL->COD_NAT,;
			qSQL->DESC_NAT,;
			qSQL->DOCUMENTO,;
			qSQL->TIPO,;
			qSQL->VENCIMENTO,;
			qSQL->CLIFOR,;
			qSQL->NOME,;
			nValor+U_NRetImpostos(),;
			cTipo,;
			qSQL->COD_CC+qSQL->COD_NAT+qSQL->VENCIMENTO,;
			qSE5->E5_DATA,;
			qSQL->E2_PREFIXO})
			nSaldo+=nValor
	  Else
         If qSE5->E5_TIPODOC = 'DC'
           nSaldo+=qSE5->E5_VALOR
         Elseif qSE5->E5_TIPODOC = 'JR' .OR. qSE5->E5_TIPODOC = 'MT'  
                nAcrescimos+=qSE5->E5_VALOR
         Endif
	  Endif  
	  DbSkip()
	  
    Enddo
	qSe5->(dbCloseArea())
	RestArea(aArea)
    If nSaldo > 0 .And. MV_PAR16 = 2  
       aDel(aPosCC,nPosAtual)
       aSize(aPosCC,Len(aPosCC)-1)
    Else
	   aPosCC[nPosAtual][10] := (aPosCC[nPosAtual][10] - nSaldo)  + nAcrescimos 
       If nSaldo > 0 
          aPosCC[nPosAtual][10] := aPosCC[nPosAtual][10] - U_NRetImpostos()
	      aPosCC[nPosAtual][13] := " " 
	   Endif   
	Endif   
	qSql->(DbSkip())	  
Enddo
Return

User Function NRetImpostos()
Local aArea := GetArea()
Local nValor := 0
Local cQuery:= ""
cQuery:=" SELECT   SUM(E2_ISS+E2_IRRF+E2_INSS+E2_COFINS+E2_PIS+E2_CSLL) AS VALOR FROM "
cQuery+="	  " + RetSqlName("SE2") + " SE2 "	
cQuery+="WHERE "
cQuery+="	    E2_FILIAL  BETWEEN '"+mv_par01+"' AND '"+mv_par02+"' AND "
cQuery+="	    E2_TIPO    ='"+aPosCC[nPosAtual][6]+"' AND "
cQuery+="	    E2_NUM     ='"+aPosCC[nPosAtual][5]+"' AND "
cQuery+="		E2_PREFIXO ='"+aPosCC[nPosAtual][14]+"' AND   " 
cQuery+="		E2_FORNECE ='"+aPosCC[nPosAtual][08]+"' AND   " 
cQuery+="		D_E_L_E_T_ <> '*' "	
cQuery := ChangeQuery(cQuery)
dbUseArea(.T., "TOPCONN", TCGenQry(,,cQuery),'qAux2', .F., .T.)
If !Eof()
   nValor := qAux2->VALOR
Endif
qAux2->(DbCloseArea())
RestArea(aArea)
Return(nValor) 
                                

User Function NVerRateios()
Local aArea := GetArea()
Local nValor := 0
Local cQuery:= ""

cQuery:=" SELECT EV_VALOR,EV_PERC,EV_RATEICC FROM " + RetSqlName("SEV") + " SEV WHERE "	
cQuery+=" EV_NUM        = '"+aPosCC[nPosAtual][5]+"'  " 
cQuery+=" AND EV_CLIFOR = '"+aPosCC[nPosAtual][08]+"'    " 
cQuery+=" AND EV_PREFIXO ='"+aPosCC[nPosAtual][14]+"'    " 
cQuery+=" AND EV_TIPO    ='"+aPosCC[nPosAtual][6]+"'      "
cQuery+=" AND EV_RECPAG = 'P'
cQuery+=" AND D_E_L_E_T_ <> '*' "	

cQuery := ChangeQuery(cQuery)
dbUseArea(.T., "TOPCONN", TCGenQry(,,cQuery),'qAux3', .F., .T.)
If !Eof()
   If qAux3->EV_RATEICC = '1'
      nValor:=U_NVerRatCC()
   Else
      nValor:=(qAux3->EV_VALOR * (qAux3->EV_PERC * 100)) / 100   // o Campo ev_valor ja o valor percentual. no precisa aplicaar novamente.
   Endif   
Endif
qAux3->(DbCloseArea())
RestArea(aArea)
Return(nValor) 


User Function NVerRatCC()
Local aArea := GetArea()
Local nValor := 0
Local cQuery:= ""

cQuery:=" SELECT EZ_VALOR,EZ_PERC FROM " + RetSqlName("SEZ") + " SEZ WHERE "	
cQuery+=" EZ_NUM        = '"+aPosCC[nPosAtual][5]+"'  " 
cQuery+=" AND EZ_CLIFOR = '"+aPosCC[nPosAtual][08]+"'    " 
cQuery+=" AND EZ_PREFIXO ='"+aPosCC[nPosAtual][14]+"'    " 
cQuery+=" AND EZ_TIPO    ='"+aPosCC[nPosAtual][6]+"'      "
cQuery+=" AND EZ_RECPAG = 'P'
cQuery+=" AND D_E_L_E_T_ <> '*' "	

cQuery := ChangeQuery(cQuery)
dbUseArea(.T., "TOPCONN", TCGenQry(,,cQuery),'qAux4', .F., .T.)
If !Eof()
   nValor:=(qAux4->EZ_VALOR * (qAux4->EZ_PERC * 100)) / 100   
Endif
qAux4->(DbCloseArea())
RestArea(aArea)
Return(nValor) 






