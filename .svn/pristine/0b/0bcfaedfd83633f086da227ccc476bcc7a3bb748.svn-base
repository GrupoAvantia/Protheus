#include "rwmake.ch"       

User Function Pagmod()     

//Autor: Ramón Barros
//Data: 10 de março de 2008
//Programa criado para atender o CNAB a pagar do Bradesco.

SetPrvt("_AMODEL,")

_aModel := SUBSTR(SEA->EA_MODELO,1,2)

IF _aModel == "  "
   IF SUBSTR(SE2->E2_CODBAR,1,3) == "237"
      _aModel := "30"
   ELSE
      _aModel := "31"
   ENDIF
ENDIF


Return(_aModel)      
