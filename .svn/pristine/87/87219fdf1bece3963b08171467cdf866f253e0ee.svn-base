Local cCabec2	:=""
Local nTotGralBas	:=	0
Local nTotGralRet	:=	0
Local lImprime		:= .F.

Local bCond1	:=	{|x| x != TMP->CFO 		}
Local bCond2	:=	{|x| x != TMP->CLIEFOR  }
Local cControle1:=  "9999" //cCFO
Local cControle2:=  "999999999" //cCCliefor
