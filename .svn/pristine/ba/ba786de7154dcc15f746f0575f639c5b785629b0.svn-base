	//-- Saldo
	CNRBox(1205,2350,RetTitle("CN9_SALDO"),TransForm((cAliasTop)->(C010xMoeda(.F.,CN9_NUMERO,CN9_REVISA,"CN9_SALDO")),cPtVLATU),.T.)

