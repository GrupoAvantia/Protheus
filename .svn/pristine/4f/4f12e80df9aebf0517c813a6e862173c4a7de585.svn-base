Function PrintGIMDF(cTXT)
	Local 		aArea		:=	GetArea ()
	Local		lRet		:=	.T.
	Local 		aTitulo  	:= 	STR0003
	Local 		cDesc1  	:= 	STR0002
	Local 		cDesc2  	:= 	""
	Local 		cDesc3  	:= 	""
	Local 		wnrel   	:= 	"MTR923"
	Local 		NomeProg	:= 	"MTR923"	
	Private 	Tamanho 	:= 	"M" 	// P/M/G
	Private 	Limite  	:= 	132	    // 80/132/220
	Private 	lEnd    	:= 	.F.		// Controle de cancelamento do relatorio
	Private 	m_pag   	:= 	1  		// Contador de Paginas
	Private		nLastKey	:= 	0  		// Controla o cancelamento da SetPrint e SetDefault
	Private 	aReturn 	:= { STR0003, 1,STR0004, 1, 2, 1, "",1 } //"Zebrado"###"Administracao"
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณEnvia para a SetPrinter                                                 ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	wnrel	:=	SetPrint ("", NomeProg, "", @aTitulo, cDesc1, cDesc2, cDesc3, .F.,, .T., Tamanho,, .F.)

	If (nLastKey==27)
		Return (lRet)
	Endif
	//
	SetDefault (aReturn, "")
	//
	If (nLastKey==27)
		Return (lRet)
	Endif
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณPreparacao do inicio de processamento do arquivo pre-formatado          ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	RptStatus({|lEnd|GeraRelGIMDF(cTXT)},aTitulo)
	//
	If (aReturn[5]==1)
		Set Printer To 	
	   	ourspool(wnrel)
	Endif
	MS_FLUSH()
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Restaura area ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	RestArea (aArea)
Return (lRet)

