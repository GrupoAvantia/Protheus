*/
Static Function GetBackColor(nLinha)

	Local nRet := CLR_WHITE
	
	If nLinha == 1 .Or. nLinha == 9 .Or. nLinha == 14 .Or. nLinha == 16
		nRet := CLR_YELLOW
	ElseIf nLinha == 2 .Or. nLinha == 8 .Or. nLinha == 10 .Or. nLinha == 13 .Or. nLinha == 15
		nRet := CLR_WHITE
	Else
		nRet := CLR_HCYAN
	EndIf
	
Return nRet


