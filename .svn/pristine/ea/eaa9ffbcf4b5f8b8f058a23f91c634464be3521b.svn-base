Function MATR985()



wnRel		:= "MATR985"
Titulo	:= If( cPaisLoc $ "ANG|PTG", "Listagem De Código S De Emitentes (mod. P10)", "Listagem de Codigos de Emitentes (Mod. P10)" )
cDesc1	:= If( cPaisLoc $ "ANG|PTG", "Emissão do registo modelo p10 conforme parâmetros", "Emissäo do Registro modelo P10 conforme parametros" )
cDesc2	:= If( cPaisLoc $ "ANG|PTG", "Informados pelo utilizador.", "informados pelo usuario." )
cDesc3	:= ""
aReturn	:= { If( cPaisLoc $ "ANG|PTG", "Código de barras", "Zebrado" ), 1,If( cPaisLoc $ "ANG|PTG", "Administração", "Administraçäo" ), 2, 2, 1, "",1 }
nomeprog	:= "MATR985"
cPerg		:= "MTR985"
cString	:= "SF3"
nPagina	:= 0
nLin		:= 80
Tamanho	:= "M"
lEnd		:= .F. 






Pergunte(cPerg, .F. )



nLastKey:=0
wnrel:=SetPrint(cString,wnrel,cPerg,Titulo,cDesc1,cDesc2,cDesc3, .F. ,"", .F. ,Tamanho,, .T. )
If nLastKey==27
	dbClearFilter()
	Return
Endif
SetDefault(aReturn,cString)
If nLastKey==27
	dbClearFilter()
	Return
Endif




RptStatus({|lEnd| R985Imp(@lEnd,wnRel,cString,Tamanho)},titulo)





dbSelectArea("SF3")

dbClearFilter()
dbSetOrder(1)
If aReturn[5]==1
	Set( 24, "" )
	ourspool(wnrel)
Endif

MS_FLUSH()

Return












Static Function R985Imp(lEnd,WnRel,cString,Tamanho)




Local cNome,cInscr,cCgc
Local cFilterUser:= aReturn[07]

Private cNomEmp	:= SM0->M0_NOMECOM
Private lAbortPrint:= .F. 
Private nLargMax	:= 132
Private nLimPag	:= 55
Private m_pag		:= 1




cEmitDe	:= mv_par01
cEmitAte	:= mv_par02
dDataIni := mv_par03
dDataFim := mv_par04




aL:=NIL
R985LayOut()




dbSelectArea("SA1")
dbSetOrder(1)

dbSelectArea("SA2")
dbSetOrder(1)




dbSelectArea("SF3")

cArq	:= CriaTrab(NIL, .F. )
cKey	:= "F3_CLIEFOR+F3_LOJA"
cFiltro := 'F3_FILIAL=="'+xFilial()+'".And.dTos(F3_ENTRADA)>="'+dTos(dDataIni)+'".And.'
cFiltro := cFiltro + 'dTos(F3_ENTRADA)<="'+dTos(dDataFim)+'".And.F3_CLIEFOR>="'+cEmitDe+'".And.'
cFiltro := cFiltro +	'F3_CLIEFOR<="'+cEmitAte+'"'

IndRegua("SF3",cArq,cKey,,cFiltro,If( cPaisLoc $ "ANG|PTG", "A seleccionar registos .. ", "Selecionando registros .. " ))
nIndex:=RetIndex("SF3")



dbSetOrder(nIndex+1)
dbGotop()

SetRegua(Reccount())

cClieFor:=""

While !Eof()

	IncRegua()
	If Interrupcao(@lAbortPrint)
		Exit
	Endif




	If (cClieFor==F3_CLIEFOR+F3_LOJA)
		dbSkip()
		Loop
	Else
		cClieFor:=F3_CLIEFOR+F3_LOJA
	Endif




	If !Empty(cFilterUser) .and. !(&cFilterUser)
		dbSkip()
		Loop
	EndIf




	cNome	:=""
	cInscr:=""
	cCgc	:=""

	If Val(Substr(F3_CFO,1,1)) > 5
		If F3_TIPO$"BD"
			dbSelectArea("SA2")
			If dbSeek(xFilial()+SF3->F3_CLIEFOR+SF3->F3_LOJA, .F. )
				cNome	:=A2_NOME
				cInscr:=A2_INSCR
				cCgc	:=A2_CGC
			Endif
		Else
			dbSelectArea("SA1")
			If dbSeek(xFilial()+SF3->F3_CLIEFOR+SF3->F3_LOJA, .F. )
				cNome	:=A1_NOME
				cInscr:=A1_INSCR
				cCgc	:=A1_CGC
			Endif
		Endif
	Else
		If F3_TIPO$"BD"
			dbSelectArea("SA1")
			If dbSeek(xFilial()+SF3->F3_CLIEFOR+SF3->F3_LOJA, .F. )
				cNome	:=A1_NOME
				cInscr:=A1_INSCR
				cCgc	:=A1_CGC
			Endif
		Else
			dbSelectArea("SA2")
			If dbSeek(xFilial()+SF3->F3_CLIEFOR+SF3->F3_LOJA, .F. )
				cNome	:=A2_NOME
				cInscr:=A2_INSCR
				cCgc	:=A2_CGC
			Endif
		Endif
	Endif

	dbSelectArea("SF3")




	If nLin>nLimPag
		R985Cabec()
	Endif

	aDados:=Array(05)

	aDados[1]:=SF3->F3_CLIEFOR
	aDados[2]:=cNome
	aDados[3]:=SF3->F3_ESTADO
	aDados[4]:=cCgc
	aDados[5]:=cInscr

	FmtLin(@aDados,aL[13],,,@nLin)

	dbSelectArea("SF3")
	dbSkip()

EndDo




R985Branco()

dbSelectArea("SF3")
cArq:=cArq+OrdBagExt()
RetIndex("SF3")
Ferase(cArq)
dbSetOrder(1)

Return











Static Function R985Cabec
Local cInscr  	:= InscrEst()
Local cCGC		:= TRANSFORM(SM0->M0_CGC,"@R 99.999.999/9999-99")
Local cData  	:= dToC(ddatabase)

nPagina := nPagina+1
cPagina := StrZero(nPagina,6)
cPagina := Right(cPagina,4)



If nPagina>1
	FmtLin(,aL[01],,,@nLin)
Endif
nLin:=0
FmtLin(,AvalImp(nLargMax),,,@nLin)
FmtLin(,{aL[01],aL[02],aL[03]},,,@nLin)
FmtLin({cNomEmp},aL[04],,,@nLin)
FmtLin(,aL[05],,,@nLin)
FmtLin({cInscr,cCGC},aL[06],,,@nLin)
FmtLin(,aL[07],,,@nLin)
FmtLin({cPagina,cData},aL[08],,,@nLin)
FmtLin(,{aL[09],aL[10],aL[11],aL[12]},,,@nLin)

Return











Static Function R985LayOut

aL:=Array(13)

aL[01]:="+----------------------------------------------------------------------------------------------------------------------------------+"
aL[02]:=If( cPaisLoc $ "ANG|PTG", "|                                                    tabela de código s de emitentes                                                |", "|                                                    TABELA DE CODIGOS DE EMITENTES                                                |" )
aL[03]:="|                                                                                                                                  |"
aL[04]:=If( cPaisLoc $ "ANG|PTG", "| firma: #############################                                                                                             |", "| FIRMA: #############################                                                                                             |" )
aL[05]:="|                                                                                                                                  |"
aL[06]:=If( cPaisLoc $ "ANG|PTG", "| insc.est: ###########           c.n.p.j.: ##############                                                                         |", "| INSC.EST: ###########           C.N.P.J.: ##############                                                                         |" )
aL[07]:="|                                                                                                                                  |"
aL[08]:=If( cPaisLoc $ "ANG|PTG", "| folha: #######                   data: ########                                                                                  |", "| FOLHA: #######                   DATA: ########                                                                                  |" )
aL[09]:="|                                                                                                                                  |"
aL[10]:="|----------+--------------------------------------------------------+------+----------------------+--------------------------------+"
aL[11]:=If( cPaisLoc $ "ANG|PTG", "|  código   | emitente                                               |  uf  |       c.n.p.j.       |  inscrição estadual            |", "|  CODIGO  | EMITENTE                                               |  UF  |       C.N.P.J.       |  INSCRICAO ESTADUAL            |" )
aL[12]:="|----------+--------------------------------------------------------+------+----------------------+--------------------------------+"
aL[13]:="|  ######  | ###################################################### |  ##  |  ##################  |  ###################           |"




Return












Static Function R985Branco

While nLin<nLimPag
	FmtLin(Array(05),aL[13],,,@nLin)
EndDo
FmtLin(,aL[01],,,@nLin)

Return