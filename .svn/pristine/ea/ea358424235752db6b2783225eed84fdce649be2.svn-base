AADD(aCampos,{ "TB_VALOR4 ","N",18,nDecs } )
AADD(aCampos,{ "TB_VALOR5 ","N",18,nDecs } )
AADD(aCampos,{ "TB_VALOR6 ","N",18,nDecs } )
