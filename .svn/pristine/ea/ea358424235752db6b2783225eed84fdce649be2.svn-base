	Endif

	If li > 55
		cabec(titulo,cabec1,cabec2,nomeprog,tamanho,nTipo)
	Endif

	If mv_par07 == 3
		If Empty(cEst)
			cEst := TB_EST
		ElseIf cEst <> TB_EST
			li++
			PrintOut(li,00,OemToAnsi(If(cPaisLoc$"ANG|PTG","Total do distrito de ","Total do Estado de "))+cEst+"--->",,)
			PrintOut(li,67,nEstV1,cPict,)
			PrintOut(li,85,nEstV2,cPict,)
			PrintOut(li,103,nEstV3,cPict,)

			If nEstv4+nEstv5+nEstv6<>0
				li++
				PrintOut(li,67,nEstV4,cPict,)
				PrintOut(li,85,nEstV5,cPict,)
				PrintOut(li,103,nEstV6,cPict,)
				PrintOut(li,129,"DEV",,)
				If nAbto == 1
					li++
					PrintOut(li,67,nEstV1+nEstV4,cPict,)
					PrintOut(li,85,nEstV2+nEstV5,cPict,)
					PrintOut(li,103,nEstV3+nEstV6,cPict,)
					PrintOut(li,129,"ABT",,)
				Endif
			Endif
			cEst := TB_EST
			li++
			nEstV1:=0
			nEstV2:=0
			nEstV3:=0
			nEstV4:=0
			nEstV5:=0
			nEstV6:=0
			li++
		EndIf
		PrintOut(li,00,OemToAnsi(If(cPaisLoc$"ANG|PTG","Distrito: ","Estado: "))+cEst,,)
		li++
	EndIf

	cCliente := TB_CLI
	cLoja    := TB_LOJA
	dbSelectArea("SA1")
	dbSetOrder(1)
	dbSeek(xFilial()+cCliente+cLoja)

	PrintOut(li,00,SA1->A1_COD+"/"+SA1->A1_LOJA,,)
	PrintOut(li,26,Substr(SA1->A1_NOME,1,40),,)

	dbSelectArea(cNomArq)

	nValor1:= TB_VALOR1
	nValor2:= TB_VALOR2
	nValor3:= TB_VALOR3
	nValor4:= TB_VALOR4
	nValor5:= TB_VALOR5
	nValor6:= TB_VALOR6

	nValor4*=(-1)
	nValor5*=(-1)
	nValor6*=(-1)

	PrintOut(li,67,nValor1,cPict,)
	PrintOut(li,85,nValor2,cPict,)
	PrintOut(li,103,nValor3,cPict,)

	IF mv_par07 = 1
		nRank:=TB_RANKIN
		PrintOut(li,124,nRank,"9999",)
	ELSEIF mv_par07 = 2
		nRank++
		PrintOut(li,124,nRank,"9999",)
	EndIF

	If nValor4+nValor5+nValor6<>0
		li++
		PrintOut(li,67,nValor4,cPict,)
		PrintOut(li,85,nValor5,cPict,)
		PrintOut(li,103,nValor6,cPict,)
		PrintOut(li,129,"DEV",,)
		If nAbto == 1
			li++
			PrintOut(li,67,nValor1+nValor4,cPict,)
			PrintOut(li,85,nValor2+nValor5,cPict,)
			PrintOut(li,103,nValor3+nValor6,cPict,)
			PrintOut(li,129,"ABT",,)
		Endif
	Endif
	nEstV1+= nValor1
	nEstV2+= nValor2
	nEstV3+= nValor3
	nEstV4+= nValor4
	nEstV5+= nValor5
	nEstV6+= nValor6

	li++

	nAg1 += nValor1
	nAg2 += nValor2
	nAg3 += nValor3
	nAg4 += nValor4
	nAg5 += nValor5
	nAg6 += nValor6

	dbSkip()
EndDo

IF li <> 80

	If li > 54
		cabec(titulo,cabec1,cabec2,nomeprog,tamanho,nTipo)
	Endif

	If mv_par07 == 3
		li++
		PrintOut(li,00,OemToAnsi(If(cPaisLoc$"ANG|PTG","Total do distrito de ","Total do Estado de "))+cEst+"--->",,)
		PrintOut(li,67,nEstV1,cPict,)
		PrintOut(li,85,nEstV2,cPict,)
		PrintOut(li,103,nEstV3,cPict,)

		If (nEstV4+nEstV5+nEstV6)<>0
			li++
			PrintOut(li,67,nEstV4,cPict,)
			PrintOut(li,85,nEstV5,cPict,)
			PrintOut(li,103,nEstV6,cPict,)
			PrintOut(li,129,"DEV",,)
			If nAbto == 1
				li++
				PrintOut(li,67,nEstV1+nEstV4,cPict,)
				PrintOut(li,85,nEstV2+nEstV5,cPict,)
				PrintOut(li,103,nEstV3+nEstV6,cPict,)
				PrintOut(li,129,"ABT",,)
			Endif
		Endif

		li:=li + 2
	EndIf
	li++
	PrintOut(li,00,OemToAnsi(If(cPaisLoc$"ANG|PTG","T o t a l --->","T O T A L --->")),,)
	PrintOut(li,67,nAg1,cPict,)
	PrintOut(li,85,nAg2,cPict,)
	PrintOut(li,103,nAg3,cPict,)

	If (nAg4+nAg5+nAg6)<>0
		li++
		PrintOut(li,67,nAg4,cPict,)
		PrintOut(li,85,nAg5,cPict,)
		PrintOut(li,103,nAg6,cPict,)
		PrintOut(li,129,"DEV",,)
		If nAbto == 1
			li++
			PrintOut(li,67,nAg1+nAg4,cPict,)
			PrintOut(li,85,nAg2+nAg5,cPict,)
			PrintOut(li,103,nAg3+nAg6,cPict,)
			PrintOut(li,129,"ABT",,)
		Endif
	Endif

	roda(cbcont,cbtxt)
EndIF

dbSelectArea(cNomArq)
dbCloseArea()

cDelArq := cNomArq+GetDBExtension()

If File(cDelArq)
	fErase(cDelArq)
Endif
fErase(cNomArq1+OrdBagExt())
If mv_par07 <> 3
	Ferase(cNomarq2+OrdBagExt())
EndIF



If mv_par09 == 3
	dbSelectArea("SD1")
	dbClearFilter()
	RetIndex()
	fErase(cNomArq3+OrdBagExt())
	dbSetOrder(1)
Endif

dbSelectArea("SF2")
