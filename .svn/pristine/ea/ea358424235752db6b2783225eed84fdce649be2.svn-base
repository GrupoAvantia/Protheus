cabec2 := OemToAnsi(If( cPaisLoc $ "ANG|PTG", "                                                                         s/impostos        mercadoria              total        ", "                                                                         S/IMPOSTOS        MERCADORIA              TOTAL        " ))








aTam:=TamSX3("F2_CLIENTE")
AADD(aCampos,{ "TB_CLI"    ,"C",aTam[1],aTam[2] } )
aTam:=TamSX3("F2_LOJA")
