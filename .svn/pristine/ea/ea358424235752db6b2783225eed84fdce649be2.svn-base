dbSelectArea(cNomArq)
dbGoTop()

While !Eof()
	IncRegua()

	If lEnd
		PrintOut(_PROW()+1,001,OemToAnsi(If(cPaisLoc$"ANG|PTG","Cancelado Pelo Operador","CANCELADO PELO OPERADOR")),,)
