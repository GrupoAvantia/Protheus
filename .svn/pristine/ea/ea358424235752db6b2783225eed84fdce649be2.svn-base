cNomArq := CriaTrab(aCampos)
dbUseArea( .T. ,, cNomArq,cNomArq, if( .T.  .OR.  .F. , ! .F. , NIL), .F.  )
cNomArq1 := SubStr(cNomArq,1,7)+"1"
cNomArq2 := SubStr(cNomArq,1,7)+"2"
cNomArq3 := SubStr(cNomArq,1,7)+"3"
cNomArq4 := SubStr(cNomArq,1,7)+"4"

IndRegua(cNomArq,cNomArq1,"TB_CLI+TB_LOJA",,,OemToAnsi(If( cPaisLoc $ "ANG|PTG", "A Seleccionar Registos...", "Selecionando Registros..." )))

dbSelectArea("SF2")
dbSetOrder(2)

SetRegua(RecCount())



if mv_par09 == 3
	cKey:="D1_FILIAL+D1_SERIORI+D1_NFORI+D1_FORNECE+D1_LOJA"
	cFiltro :="D1_FILIAL=='"+xFilial("SD1")+"'.And.!Empty(D1_NFORI)"
	cFiltro += ".And. !("+IsRemito(2,"SD1->D1_TIPODOC")+")"




	IndRegua("SD1",cNomArq3,cKey,,cFiltro,OemToAnsi(If( cPaisLoc $ "ANG|PTG", "A Seleccionar Registos...", "Selecionando Registros..." )))
	nIndex:=RetIndex("SD1")



	dbSetOrder(nIndex+1)
	dbGotop()
