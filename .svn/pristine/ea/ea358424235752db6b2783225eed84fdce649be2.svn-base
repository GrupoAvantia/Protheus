dbSelectArea(cNomArq)
dbCommit()
dbClearIndex()

If mv_par07 == 1

	IndRegua(cNomArq,cNomArq2,"StrZero(1000000000000 - TB_VALOR3 + TB_VALOR6,18,"+Str(nDecs)+")",,,OemToAnsi(If( cPaisLoc $ "ANG|PTG", "A Seleccionar Registos...", "Selecionando Registros..." )))
	dbGoTop()
	nRank:=1
	While !Eof()
		RecLock(cNomArq, .F. )
		_FIELD->TB_RANKIN := nRank
		MsUnlock()
		nRank++
		dbSkip()
	Enddo
	nRank:=0

	dbClearIndex()
	IndRegua(cNomArq,cNomArq1,"TB_CLI+TB_LOJA",,,OemToAnsi(If( cPaisLoc $ "ANG|PTG", "A Seleccionar Registos...", "Selecionando Registros..." )))
