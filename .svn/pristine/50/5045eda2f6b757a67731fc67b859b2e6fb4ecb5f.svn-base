	Local nX		:=	0







PRIVATE aLinha:={}



pergunte(cPerg, .F. )








PRIVATE m_pag:=IIf(mv_par03<=0,1,mv_par03)
PRIVATE nQtFeixe:=IIF(mv_par04<=0,500,mv_par04)

For nForFilial := 1 to Len(aFilsCalc)

	If aFilsCalc[nForFilial][1]

