Static Function CNRBox(nCol1,nCol2,cTexto1,cTexto2,lQuebra,nTipo,oFontB,lAlinDir)
//Ŀ
// Inicializa Fontes                                            
//
Local oFont10		:= TFont():New("Arial",10,10,,.F.,,,,.T.,.F.)
Local oFont08n	:= TFont():New("Arial",08,08,,.T.,,,,.T.,.F.)

Default nCol1	:= 0
Default nCol2	:= 0
Default cTexto1 := ""
Default cTexto2 := ""
Default lQuebra := .T.
Default nTipo   := 3
Default oFontB  := oFont10
Default lAlinDir:= .F.

If nLin >= 3000
	CNRCabec(Nil,Nil,OemToAnsi(cTitRel)) //-- Impressao do Cabecalho da pagina
EndIf

If nTipo == 1  //-- Linha de Cabecalho
	oPrint:Box(nLin,nCol1,nLin+55,nCol2)
	oPrint:Box(nLin+5,nCol1+5,nLin+50,nCol2-5)
	oPrint:Say(nLin+10,((nCol2-nCol1)/2)-((Len(alltrim(cTexto1))/2)*20),cTexto1,oFont08n)
ElseIf nTipo == 2 //-- Linha Unica Simples
	oPrint:Box(nLin,nCol1,nLin+45,nCol2)
	If lAlinDir //-- Alinhamento a Direita
		oPrint:Say(nLin+7,nCol2-5-17.241*Len(Alltrim(cTexto1)),AllTrim(cTexto1),oFontB)
	Else 
		oPrint:Say(nLin+7,nCol1+10,cTexto1,oFontB)
	EndIf
ElseIf nTipo == 3 //-- Linha de Detalhe
	oPrint:Box(nLin,nCol1,nLin+90,nCol2)
	oPrint:Say(nLin+10,nCol1+10,cTexto1,oFontB)
	If lAlinDir //-- Alinhamento a Direita
		oPrint:Say(nLin+50,nCol2-5-17.241*Len(Alltrim(cTexto2)),AllTrim(cTexto2),oFontB)
	Else
		oPrint:Say(nLin+50,nCol1+10,cTexto2,oFontB)
	EndIf
ElseIf nTipo == 4 //-- Somente Texto
	//oPrint:Say(nLin+7,nCol1,cTexto1,oFontB)
	If lAlinDir //-- Alinhamento a Direita
		oPrint:Say(nLin+7,nCol2-5-17.241*Len(Alltrim(cTexto1)),AllTrim(cTexto1),oFontB)
	Else 
		oPrint:Say(nLin+7,nCol1+10,cTexto1,oFontB)
	EndIf
ElseIf nTipo == 5 //-- Descrio Detalhada 3 linhas
	oPrint:Box(nLin,nCol1,nLin+135,nCol2)
	oPrint:Say(nLin+10,nCol1+10,cTexto1,oFontB)
	If lAlinDir //-- Alinhamento a Direita
		oPrint:Say(nLin+50,nCol2-5-17.241*Len(Alltrim(Substr(cTexto2,1,50))),AllTrim(Substr(cTexto2,1,50)),oFontB)
		oPrint:Say(nLin+90,nCol2-5-17.241*Len(Alltrim(Substr(cTexto2,51,150))),AllTrim(Substr(cTexto2,51,150)),oFontB)
	Else
		oPrint:Say(nLin+50,nCol1+10,Substr(cTexto2,1,50),oFontB)
		oPrint:Say(nLin+90,nCol1+10,Substr(cTexto2,51,150),oFontB)
	EndIf
ElseIf nTipo == 6 //-- Descrio Detalhada 4 linhas
	oPrint:Box(nLin,nCol1,nLin+180,nCol2)
	oPrint:Say(nLin+10,nCol1+10,cTexto1,oFontB)
	If lAlinDir //-- Alinhamento a Direita
		oPrint:Say(nLin+50,nCol2-5-17.241*Len(Alltrim(Substr(cTexto2,1,50))),AllTrim(Substr(cTexto2,1,50)),oFontB)
		oPrint:Say(nLin+90,nCol2-5-17.241*Len(Alltrim(Substr(cTexto2,51,50))),AllTrim(Substr(cTexto2,51,50)),oFontB)
		oPrint:Say(nLin+135,nCol2-5-17.241*Len(Alltrim(Substr(cTexto2,101,50))),AllTrim(Substr(cTexto2,101,50)),oFontB)
	Else
		oPrint:Say(nLin+50,nCol1+10,Substr(cTexto2,1,50),oFontB)
		oPrint:Say(nLin+90,nCol1+10,Substr(cTexto2,51,50),oFontB)
		oPrint:Say(nLin+135,nCol1+10,Substr(cTexto2,101,50),oFontB)
	EndIf
EndIf
If lQuebra //-- Quebra Linha
	If nTipo == 1
		nLin += 50	//-- Cabecalho
	ElseIf nTipo == 2
		nLin += 50	//-- Unica Simples
	ElseIf nTipo == 3
		nLin += 95	//-- Detalhe
	ElseIf nTipo == 4 
		nLin += 50 //-- Somente Texto
	ElseIf nTipo == 5 
		nLin += 140 //-- Descrio estendida 3 linhas
	ElseIf nTipo == 6 
		nLin += 185 //-- Descrio estendida 4 linhas
	EndIf
EndIf	

Return

/*/


Ŀ
Funao     TamTexto | Autor  Marcos V. Ferreira     Data 08/03/2006
Ĵ
Descrio  Recupera o tamanho de uma string em pixels                 
Ĵ
 Uso       CNTR010 / CNTR020                                          
ٱ


/*/

Static Function TamTexto(cTexto)
Local nSize := 0
Local nx
Local aSize := {{17.241,2.692,15.241,13.692}} 
For nx := 1 to Len(cTexto)
	If Substr(cTexto,nx,1)=="." .Or. Substr(cTexto,nx,1)=="," .or.  Substr(UPPER(cTexto),nx,1)=="I" 
		nSize += aSize[1,2]
	ElseIf Substr(cTexto,nx,1)$"abcdefghijklmnopqrstuvxyzw"
		nSize += aSize[1,4]
	ElseIf Substr(UPPER(cTexto),nx,1)$"ABCDEFGHIJKLMNOPQRSTUVXWYZ""
		nSize += aSize[1,3]
	Else                     
		nSize += aSize[1,1]
	EndIf
Next
Return nSize


