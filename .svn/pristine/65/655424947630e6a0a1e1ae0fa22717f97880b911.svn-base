#INCLUDE "PROTHEUS.CH" 
#INCLUDE "RWMAKE.CH"   
#INCLUDE "TOPCONN.CH" 

/*


Ŀ
Funcao    AVPMSR01   Autor  Grayson B.Tavares      Data  28.07.09 
Ĵ
Descricao Relatorio Relao de Insumos                                
Ĵ
Uso       Gestao de Projetos                                          
Ĵ


*/
User Function AVPMSR03()

Local cMens := ""
Local nOpca := 0

Private cTitRel := OemToAnsi("Curva ABC de Insumos por Tipo")
Private lEnd	:= .F.

//Ŀ
// Ajustar perguntas do SX1                                     
//
AjustaSX1("APMSR03")

Pergunte("APMSR03",.F.)

//Ŀ
// Tela de configuracao do Relatorio                            
//
DEFINE MSDIALOG oDlg FROM  96,9 TO 310,592 TITLE OemToAnsi("Curva ABC de Insumos por Tipo") PIXEL    //"Contratos"
@ 18, 6 TO 66, 287 LABEL "" OF oDlg  PIXEL
@ 29, 15 SAY OemToAnsi("Este programa ira emitir a Curva ABC de Insumos por Tipo") SIZE 268, 8 OF oDlg PIXEL
@ 38, 15 SAY OemToAnsi("exibindo os dados de acordo com os parametros informados") SIZE 268, 8 OF oDlg PIXEL
@ 48, 15 SAY OemToAnsi("") SIZE 268, 8 OF oDlg PIXEL
DEFINE SBUTTON FROM 80, 190 TYPE 5 ACTION Pergunte("APMSR03",.t.) ENABLE OF oDlg
DEFINE SBUTTON FROM 80, 223 TYPE 1 ACTION (oDlg:End(),nOpca:=1) ENABLE OF oDlg
DEFINE SBUTTON FROM 80, 255 TYPE 2 ACTION oDlg:End() ENABLE OF oDlg
ACTIVATE MSDIALOG oDlg CENTER

If nOpca == 1
	//Ŀ
	// Processamento do Relatorio									        
	//
	RptStatus({|lEnd| APMS03Imp(@lEnd)})
EndIf	

Return Nil

/*


Ŀ
Funo    APMS01Imp  Autor  Grayson B.Tavares      Data  28.07.09 
Ĵ
Descrio  Chamada do Relatorio                                       
Ĵ
 Uso       AVPMSR01 		                                              
ٱ


*/
Static Function APMS03Imp(lEnd)

Local cQuery 	:= ""
Local cSeek	:= ""
Local nX		:= 0
Local cDirSpool := GetMv("MV_RELT")

//Ŀ
// Inicializa Fontes                                            
//
Private oFont06	:= TFont():New("Arial"	,06,06,,.T.,,,,.T.,.F.)
Private oFont08	:= TFont():New("Courier"	,07,07,,.F.,,,,.T.,.F.)
Private oFont07n	:= TFont():New("Courier"	,07,07,,.T.,,,,.T.,.F.)
Private oFont08	:= TFont():New("Courier"	,08,08,,.F.,,,,.T.,.F.)
Private oFont08n	:= TFont():New("Courier"	,08,08,,.T.,,,,.T.,.F.)

//Ŀ
// Variaveis utilizadas pelo Objeto TMSPrinter					  
//
Private nMaxLin 	:= 3000
Private nlin 		:= nMaxLin
Private nPagina 	:= 0
Private oPrint

//Ŀ
// Variaveis Gerais                                             
//

//Ŀ
// Inicializa Objeto TMSPrinter                                 
//
oPrint:=TMSPrinter():New(OemToAnsi("Curva ABC de Insumos por Tipo"))
oPrint:SetPortrait()	//-- Retrato //SetLandscape()//paisagem
oPrint:Setup() 		//-- Configurar Impressao

//Ŀ
// Altera o titulo de acordo com o parametro selecionado        
//
cTitRel := If(mv_par05 == 1, OemToAnsi("Curva ABC de Insumos por Tipo"), OemToAnsi("Curva ABC de Insumos (Geral)"))

//Ŀ
// Query do Contrato                                            
//
cAliasTop := "XAF1"

cQuery := "SELECT AF1.*, AF5_TOTCUS "
cQuery += "FROM " + RetSqlName("AF1") + " AF1 " 
cQuery += "INNER JOIN " + RetSqlName("AF5") + " AF5 "
cQuery += "ON AF1_FILIAL = AF5_FILIAL "
cQuery += "AND AF1_ORCAME = AF5_ORCAME " 
cQuery += "WHERE AF1.D_E_L_E_T_ <> '*' "
cQuery += "AND AF5.D_E_L_E_T_ <> '*' "
cQuery += "AND AF5_NIVEL = '001' "
cQuery += "AND AF1_FILIAL =  '"		+	xFilial("AF1")	+ "' "
cQuery += "AND AF1_ORCAME >= '"		+	mv_par01			+ "' "
cQuery += "AND AF1_ORCAME <= '"		+	mv_par02			+ "' "
cQuery += "AND AF1_DATA >= '"		+	dtos(mv_par03)	+ "' "
cQuery += "AND AF1_DATA <= '"		+	dtos(mv_par04)	+ "' "
cQuery += "ORDER BY AF1_ORCAME "

cQuery := ChangeQuery(cQuery)

MsAguarde({|| dbUseArea(.T.,"TOPCONN",TCGenQry(,,cQuery),cAliasTop,.F.,.T.)},OemToAnsi("Processando"))

dbSelectArea(cAliasTop)
SetRegua(LastRec())

nLin := nMaxLin //-- Forca a Quebra da pagina por oramento
nAjus := 10

(cAliasTop)->(dbGoTop())

Do While (cAliasTop)->(!Eof())
	
	IncProc()
	
	If lEnd
		CNRBox(0050,2350,OemToAnsi("CANCELADO PELO OPERADOR"),,.T.,1)
		Exit
	EndIf

	If nLin >= nMaxLin
		CNRCabec(Nil,Nil,OemToAnsi(cTitRel),.t.) //-- Impressao do Cabecalho da pagina
	EndIf
		
	// Calculando o Valor Total
	cQryMA:="SELECT ROUND(ROUND(SUM(CAST(AF3_QUANT AS DECIMAL (10,4)) * CAST(AF2_QUANT AS DECIMAL (10,4))),2,1) * AVG(AF3_CUSTD),2,1) AS TOTAL "
	cQryMA+="FROM " + RetSqlName("AF3") + " AF3 "
	cQryMA+="INNER JOIN " + RetSqlName("AF2") + " AF2 "
	cQryMA+="ON AF3_FILIAL = AF2_FILIAL "
	cQryMA+="AND AF3_ORCAME = AF2_ORCAME "
	cQryMA+="AND AF3_TAREFA = AF2_TAREFA "
	cQryMA+="WHERE AF3_RECURS = '' "
	cQryMA+="AND AF3.D_E_L_E_T_ <> '*' "
	cQryMA+="AND AF2.D_E_L_E_T_ <> '*' "
	cQryMA+="AND AF3_ORCAME = '"	+ (cAliasTop)->AF1_ORCAME	+ "' "
	cQryMA+="AND AF3_FILIAL =  '"	+ xFilial("AF3") 				+ "' "
	cQryMA+="GROUP BY AF3_PRODUT "
	cQryMA+="UNION "
	cQryMA+="SELECT SUM(CAST(AF3_QUANT AS DECIMAL (12,6)) * CAST(AF2_QUANT AS DECIMAL (12,6)) * CAST(AF3_CUSTD AS DECIMAL (12,6))) AS TOTAL "
	cQryMA+="FROM " + RetSqlName("AF3") + " AF3 "
	cQryMA+="INNER JOIN " + RetSqlName("AF2") + " AF2 "
	cQryMA+="ON AF3_FILIAL = AF2_FILIAL "
	cQryMA+="AND AF3_ORCAME = AF2_ORCAME "
	cQryMA+="AND AF3_TAREFA = AF2_TAREFA "
	cQryMA+="WHERE AF3_RECURS <> '' "
	cQryMA+="AND AF3.D_E_L_E_T_ <> '*' "
	cQryMA+="AND AF2.D_E_L_E_T_ <> '*' "
	cQryMA+="AND AF3_ORCAME = '"	+ (cAliasTop)->AF1_ORCAME	+ "' "
	cQryMA+="AND AF3_FILIAL =  '"	+ xFilial("AF3")				+ "' "
	cQryMA+="GROUP BY AF3_RECURS "
	cQryMA+="UNION "
	cQryMA+="SELECT SUM(AF4_QUANT*AF2_QUANT*AF4_VLRUNI) TOTAL "
	cQryMA+="FROM " + RetSqlName("AF4") + " AF4 "
	cQryMA+="INNER JOIN " + RetSqlName("AF2") + " AF2 "
	cQryMA+="ON AF4_FILIAL = AF2_FILIAL "
	cQryMA+="AND AF4_ORCAME = AF2_ORCAME "
	cQryMA+="AND AF4_TAREFA = AF2_TAREFA "
	cQryMA+="WHERE AF4.D_E_L_E_T_ <> '*' "
	cQryMA+="AND AF2.D_E_L_E_T_ <> '*' "
	cQryMA+="AND AF4_ORCAME = '"	+ (cAliasTop)->AF1_ORCAME	+ "' " 
	cQryMA+="AND AF4_FILIAL =  '"	+ xFilial("AF4")				+ "' "
	cQryMA+="GROUP BY AF4_TIPOD "
	cQryMA+="ORDER BY TOTAL DESC "
	
	dbUseArea(.T.,"TOPCONN",TCGenQry(,,cQryMA),"XMA",.F.,.T.)
	
	nTotal := 0
	nTotPct:= 0
	
	While XMA->(!EOF())
		nTotal += XMA->TOTAL
		XMA->(dbSkip())
	EndDo
	
	dbSelectArea("XMA")
	dbCloseArea()
	
	If mv_par05 == 1
		//Materiais
		cQryMA:="SELECT AF3_PRODUT, SUM(CAST(AF3_QUANT AS DECIMAL (10,4)) * CAST(AF2_QUANT AS DECIMAL (10,4))) QUANT, AVG(AF3_CUSTD) CUSTD, 'MA' TIPO, SUM(CAST(AF3_QUANT AS DECIMAL (10,4)) * CAST(AF2_QUANT AS DECIMAL (10,4))) * AVG(AF3_CUSTD) AS TOTAL "
		cQryMA+="FROM "+RetSqlName("AF3")+" AF3 "
		cQryMA+="INNER JOIN "+RetSqlName("AF2")+" AF2 "
		cQryMA+="ON AF3.AF3_FILIAL = AF2.AF2_FILIAL "
		cQryMA+="AND AF3.AF3_ORCAME = AF2.AF2_ORCAME "
		cQryMA+="AND AF3_TAREFA = AF2_TAREFA "
		cQryMA+="WHERE AF3.D_E_L_E_T_ <> '*' "
		cQryMA+="AND AF2.D_E_L_E_T_ <> '*' "
		cQryMA+="AND AF3_RECURS = '' "
		cQryMA+="AND AF3_ORCAME = '"	+	(cAliasTop)->AF1_ORCAME	+ "' " 
		cQryMA+="AND AF3_FILIAL = '"	+	xFilial("AF3")			+ "' "
		cQryMA+="GROUP BY AF3_PRODUT "
		cQryMA+="ORDER BY TOTAL DESC "
		
		dbUseArea(.T.,"TOPCONN",TCGenQry(,,cQryMA),"XMA",.F.,.T.)
		
		nTotMa := 0
		lImpGrp:= .T.
		
		While XMA->(!EOF())
			nTotMa += XMA->TOTAL
			XMA->(dbSkip())
		EndDo
		
		XMA->(dbGoTop())
		
		While XMA->(!EOF())
			
			If nLin >= nMaxLin
				CNRCabec(Nil,Nil,OemToAnsi(cTitRel),.t.) //-- Impressao do Cabecalho da pagina
			EndIf
			
			cCod   := XMA->AF3_PRODUT
			cDescri:= AllTrim(Posicione("SB1",1,xFilial("SB1")+XMA->AF3_PRODUT,"B1_CEME"))			
			cUM    := Posicione("SB1",1,xFilial("SB1")+XMA->AF3_PRODUT,"B1_UM")
			
			If lImpGrp			
				CNRBox(0050,2450,"MA Materiais"											,""	,.T.,2,oFont08n,.F.)
				lImpGrp := .F.
			Else
				nTotPct+= XMA->TOTAL/nTotal*100
				If Len(cDescri) < 51
					CNRBox(0050,0335,cCod 												,	,.F.,2,oFont08,.F.)
					CNRBox(0335,1205,cDescri												,	,.F.,2,oFont08,.F.)
					CNRBox(1205,1295,cUM 												, 	,.F.,2,oFont08,.F.)
					CNRBox(1295,1580,Transform(XMA->CUSTD,"@E 999,999,999.99") 		,""	,.F.,2,oFont08,.T.)
					CNRBox(1580,1865,Transform(XMA->QUANT,"@E 999,999,999.9999") 	,""	,.F.,2,oFont08,.T.)
					CNRBox(1865,2150,Transform(XMA->TOTAL,"@E 999,999,999.99") 		,""	,.F.,2,oFont08,.T.)
					CNRBox(2150,2450,Transform(XMA->TOTAL/nTotal*100,"@E 999.999%")+" "+Transform(nTotPct,"@E 999.999%"),""    ,.T.,2,oFont07n,.F.)
				Else
					If Len(cDescri) < 101
						CNRBox(0050,0335,cCod					, 													,.F.,3,oFont08,.F.)
						CNRBox(0335,1205,Substr(cDescri,1,50)	,Substr(cDescri,51,100)							,.F.,3,oFont08,.F.)
						CNRBox(1205,1295,cUM						, 													,.F.,3,oFont08,.F.)
						CNRBox(1295,1580,""						,Transform(XMA->CUSTD,"@E 999,999,999.99") 	,.F.,3,oFont08,.T.)
						CNRBox(1580,1865,""						,Transform(XMA->QUANT,"@E 999,999,999.9999") 	,.F.,3,oFont08,.T.)
						CNRBox(1865,2150,""						,Transform(XMA->TOTAL,"@E 999,999,999.99") 	,.F.,3,oFont08,.T.)
						CNRBox(2150,2450,""						,Transform(XMA->TOTAL/nTotal*100,"@E 999.999%")+" "+Transform(nTotPct,"@E 999.999%")    ,.T.,3,oFont07n,.F.)
					Else
						If Len(cDescri) < 151
							CNRBox(0050,0335,cCod					, 													,.F.,5,oFont08,.F.)
							CNRBox(0335,1205,Substr(cDescri,1,50)	,Substr(cDescri,51,150)							,.F.,5,oFont08,.F.)
							CNRBox(1205,1295,cUM						, 													,.F.,5,oFont08,.F.)
							CNRBox(1295,1580,""						,Transform(XMA->CUSTD,"@E 999,999,999.99") 	,.F.,5,oFont08,.T.)
							CNRBox(1580,1865,""						,Transform(XMA->QUANT,"@E 999,999,999.9999") 	,.F.,5,oFont08,.T.)
							CNRBox(1865,2150,""						,Transform(XMA->TOTAL,"@E 999,999,999.99") 	,.F.,5,oFont08,.T.)
							CNRBox(2150,2450,""						,Transform(XMA->TOTAL/nTotal*100,"@E 999.999%")+" "+Transform(nTotPct,"@E 999.999%")    ,.T.,5,oFont07n,.F.)
						Else
							CNRBox(0050,0335,cCod					, 													,.F.,6,oFont08,.F.)
							CNRBox(0335,1205,Substr(cDescri,1,50)	,Substr(cDescri,51,200)							,.F.,6,oFont08,.F.)
							CNRBox(1205,1295,cUM						, 													,.F.,6,oFont08,.F.)
							CNRBox(1295,1580,""						,Transform(XMA->CUSTD,"@E 999,999,999.99") 	,.F.,6,oFont08,.T.)
							CNRBox(1580,1865,""						,Transform(XMA->QUANT,"@E 999,999,999.9999") 	,.F.,6,oFont08,.T.)
							CNRBox(1865,2150,""						,Transform(XMA->TOTAL,"@E 999,999,999.99") 	,.F.,6,oFont08,.T.)
							CNRBox(2150,2450,""						,Transform(XMA->TOTAL/nTotal*100,"@E 999.999%")+" "+Transform(nTotPct,"@E 999.999%")    ,.T.,6,oFont07n,.F.)
						EndIf
					EndIf
				EndIf
				
				XMA->(dbSkip())
			EndIf	
			
		EndDo
		
		dbSelectArea("XMA")
		dbCloseArea()
		
	    CNRBox(0335,1205,"TOTAL MATERIAIS"                    ,                     ,.F.,4,oFont08n,.F.)
	    CNRBox(1865,2150,Transform(nTotMA,"@E 999,999,999.99"),""                   ,.T.,4,oFont08n,.T.)	
		
		//Mao de obra
		cQryMA:="SELECT AF3_RECURS, SUM(CAST(AF3_QUANT AS DECIMAL (12,6)) * CAST(AF2_QUANT AS DECIMAL (12,6))) QUANT, AVG(AF3_CUSTHR) CUSTD, 'MO' TIPO, SUM(CAST(AF3_QUANT AS DECIMAL (12,6)) * CAST(AF2_QUANT AS DECIMAL (12,6)) * CAST(AF3_CUSTD AS DECIMAL (12,6))) AS TOTAL "
		cQryMA+="FROM "+RetSqlName("AF3")+" AF3 "
		cQryMA+="INNER JOIN "+RetSqlName("AF2")+" AF2 "
		cQryMA+="ON AF3.AF3_FILIAL = AF2.AF2_FILIAL "
		cQryMA+="AND AF3.AF3_ORCAME = AF2.AF2_ORCAME "
		cQryMA+="AND AF3_TAREFA = AF2_TAREFA "
		cQryMA+="WHERE AF3.D_E_L_E_T_ <> '*' "
		cQryMA+="AND AF2.D_E_L_E_T_ <> '*' "
		cQryMA+="AND AF3_RECURS <> '' "
		cQryMA+="AND AF3_ORCAME = '"	+	(cAliasTop)->AF1_ORCAME	+ "' "
		cQryMA+="AND AF3_FILIAL = '"	+	xFilial("AF3")			+ "' "
		cQryMA+="GROUP BY AF3_PRODUT,AF3_RECURS "
		cQryMA+="ORDER BY TOTAL DESC "
		
		dbUseArea(.T.,"TOPCONN",TCGenQry(,,cQryMA),"XMA",.F.,.T.)
		
		nTotMO := 0
		nTotHr := 0
		lImpGrp:= .T.
		
		While XMA->(!EOF())
			nTotMO += XMA->TOTAL
			XMA->(dbSkip())
		EndDo
		
		XMA->(dbGoTop())
		
		While XMA->(!EOF())
			
			If nLin >= nMaxLin
				CNRCabec(Nil,Nil,OemToAnsi(cTitRel),.t.) //-- Impressao do Cabecalho da pagina
			EndIf
			
			cCod   := XMA->AF3_RECURS
			cDescri:= Posicione("AE8",1,xFilial("AE8")+XMA->AF3_RECURS,"AE8_DESCRI")
			cUM    := "H"
			
			If lImpGrp			
				CNRBox(0050,2450,"MO Mao de Obra",""	 												,.T.,2,oFont08n,.F.)
				lImpGrp := .F.
			Else
				nTotPct+= XMA->TOTAL/nTotal*100
				CNRBox(0050,0335,cCod 												, 					,.F.,2,oFont08,.F.)
				CNRBox(0335,1205,cDescri 											, 					,.F.,2,oFont08,.F.)
				CNRBox(1205,1295,cUM 												, 					,.F.,2,oFont08,.F.)
				CNRBox(1295,1580,Transform(XMA->CUSTD,"@E 999,999,999.99") 		,"" 				,.F.,2,oFont08,.T.)
				CNRBox(1580,1865,Transform(XMA->QUANT,"@E 999,999,999.9999") 	,"" 				,.F.,2,oFont08,.T.)
				CNRBox(1865,2150,Transform(XMA->TOTAL,"@E 999,999,999.99") 		,"" 				,.F.,2,oFont08,.T.)
				CNRBox(2150,2450,Transform(XMA->TOTAL/nTotal*100,"@E 999.999%")+" "+Transform(nTotPct,"@E 999.999%"),""    ,.T.,2,oFont07n,.F.)
				XMA->(dbSkip())
			EndIf
			
			nTotHr+=XMA->QUANT
			
		EndDo
		
		dbSelectArea("XMA")
		dbCloseArea()    
	    
	    CNRBox(0335,1205,"TOTAL MAO DE OBRA"                  ,                     ,.F.,4,oFont08n,.F.)
	    CNRBox(1580,1865,Transform(nTotHr,"@E 999,999,999.99"),""                   ,.F.,4,oFont08n,.T.)
	    CNRBox(1865,2150,Transform(nTotMO,"@E 999,999,999.99"),""                   ,.T.,4,oFont08n,.T.)
	    
	    //Despesas
		cQryMA:="SELECT AF4_TIPOD, AF4_DESCRI, AF4_UM, SUM(AF4_QUANT*AF2_QUANT) QUANT, AVG(AF4_VLRUNI) VLRUNI "
		cQryMA+="FROM "+RetSqlName("AF4")+" AF4 "
		cQryMA+="INNER JOIN " + RetSqlName("AF2") + " AF2 "
		cQryMA+="ON AF4_FILIAL = AF2_FILIAL "
		cQryMA+="AND AF4_ORCAME = AF2_ORCAME "
		cQryMA+="AND AF4_TAREFA = AF2_TAREFA "
		cQryMA+="WHERE AF4.D_E_L_E_T_ <> '*' "
		cQryMA+="AND AF2.D_E_L_E_T_ <> '*' "
		cQryMA+="AND AF4_ORCAME = '"	+(cAliasTop)->AF1_ORCAME	+ "' "
		cQryMA+="AND AF4_FILIAL = '"	+	xFilial("AF4")		+ "' "
		cQryMA+="GROUP BY AF4_TIPOD, AF4_DESCRI, AF4_UM "
		cQryMA+="ORDER BY QUANT DESC "
		
		dbUseArea(.T.,"TOPCONN",TCGenQry(,,cQryMA),"XMA",.F.,.T.)
		
		nTotDP := 0
		lImpGrp:=.t.
		
		While XMA->(!EOF())
			nTotDP += XMA->QUANT*XMA->VLRUNI
			XMA->(dbSkip())
		EndDo
		
		XMA->(dbGoTop())
		
		While XMA->(!EOF())
			
			If nLin >= nMaxLin
				CNRCabec(Nil,Nil,OemToAnsi(cTitRel),.t.) //-- Impressao do Cabecalho da pagina
			EndIf
			
			cCod   :=XMA->AF4_TIPOD
			cDescri:=XMA->AF4_DESCRI
			cUM    :=XMA->AF4_UM
			nTotPct+=(XMA->QUANT*XMA->VLRUNI)*100/nTotDP
			
			If lImpGrp			
				CNRBox(0050,2450,"DP Despesas"   ,cCod                                                 		,.F.,2,oFont08n,.F.)
				lImpGrp := .F.
			Else
				CNRBox(0050,0335,cCod                                                		,                 ,.F.,2,oFont08,.F.)
				CNRBox(0335,1205,cDescri                                             		,                 ,.F.,2,oFont08,.F.)
				CNRBox(1205,1295,cUM                                                 		,                 ,.F.,2,oFont08,.F.)
				CNRBox(1295,1580,Transform(XMA->VLRUNI,"@E 999,999,999.99")           	,""               ,.F.,2,oFont08,.T.)
				CNRBox(1580,1865,Transform(XMA->QUANT,"@E 999,999,999.99")            	,""               ,.F.,2,oFont08,.T.)
				CNRBox(1865,2150,Transform(XMA->QUANT*XMA->VLRUNI,"@E 999,999,999.99") 	,""               ,.F.,2,oFont08,.T.)
				CNRBox(2150,2450,Transform((XMA->QUANT*XMA->VLRUNI)*100/nTotDP,"@E 999.999%")+" "+Transform(nTotPct,"@E 999.999%"),""    ,.T.,2,oFont07n,.F.)
				XMA->(dbSkip())
			EndIf
			
		EndDo
		
		dbSelectArea("XMA")
		dbCloseArea()
		    
	    CNRBox(0335,1205,"TOTAL DESPESAS"                     ,                     ,.F.,4,oFont08n,.F.)
	    CNRBox(1865,2150,Transform(nTotDP,"@E 999,999,999.99"),""                   ,.T.,4,oFont08n,.T.)
	                                    
	    CNRBox(0335,1205,"TOTAL GERAL"                        ,                     ,.F.,4,oFont08n,.F.)
	    CNRBox(1865,2150,Transform(nTotMA+nTotMO+nTotDP,"@E 999,999,999.99"),""     ,.T.,4,oFont08n,.T.)		
	Else
		cQryMA:="SELECT AF3_PRODUT COD , ROUND(SUM(CAST(AF3_QUANT AS DECIMAL (10,4)) * CAST(AF2_QUANT AS DECIMAL (10,4))),2,1) QUANT, AVG(AF3_CUSTD) VLRUNI, 'MA' TIPO, ROUND(ROUND(SUM(CAST(AF3_QUANT AS DECIMAL (10,4)) * CAST(AF2_QUANT AS DECIMAL (10,4))),2,1) * AVG(AF3_CUSTD),2,1) AS TOTAL "
		cQryMA+="FROM " + RetSqlName("AF3") + " AF3 "
		cQryMA+="INNER JOIN " + RetSqlName("AF2") + " AF2 "
		cQryMA+="ON AF3_FILIAL = AF2_FILIAL "
		cQryMA+="AND AF3_ORCAME = AF2_ORCAME "
		cQryMA+="AND AF3_TAREFA = AF2_TAREFA "
		cQryMA+="WHERE AF3_RECURS = '' "
		cQryMA+="AND AF3.D_E_L_E_T_ <> '*' "
		cQryMA+="AND AF2.D_E_L_E_T_ <> '*' "
		cQryMA+="AND AF3_ORCAME = '"	+ (cAliasTop)->AF1_ORCAME	+ "' "
		cQryMA+="AND AF3_FILIAL =  '"	+ xFilial("AF3") 				+ "' "
		cQryMA+="GROUP BY AF3_PRODUT "
		cQryMA+="UNION "
		cQryMA+="SELECT AF3_RECURS COD, SUM(CAST(AF3_QUANT AS DECIMAL (12,6)) * CAST(AF2_QUANT AS DECIMAL (12,6))) QUANT, AVG(AF3_CUSTHR) VLRUNI, 'MO' TIPO, SUM(CAST(AF3_QUANT AS DECIMAL (12,6)) * CAST(AF2_QUANT AS DECIMAL (12,6)) * CAST(AF3_CUSTD AS DECIMAL (12,6))) AS TOTAL "
		cQryMA+="FROM " + RetSqlName("AF3") + " AF3 "
		cQryMA+="INNER JOIN " + RetSqlName("AF2") + " AF2 "
		cQryMA+="ON AF3_FILIAL = AF2_FILIAL "
		cQryMA+="AND AF3_ORCAME = AF2_ORCAME "
		cQryMA+="AND AF3_TAREFA = AF2_TAREFA "
		cQryMA+="WHERE AF3_RECURS <> '' "
		cQryMA+="AND AF3.D_E_L_E_T_ <> '*' "
		cQryMA+="AND AF2.D_E_L_E_T_ <> '*' "
		cQryMA+="AND AF3_ORCAME = '"	+ (cAliasTop)->AF1_ORCAME	+ "' "
		cQryMA+="AND AF3_FILIAL =  '"	+ xFilial("AF3")				+ "' "
		cQryMA+="GROUP BY AF3_RECURS "
		cQryMA+="UNION "
		cQryMA+="SELECT AF4_TIPOD COD, SUM(AF4_QUANT*AF2_QUANT) QUANT, AVG(AF4_VLRUNI) VLRUNI, 'DP' TIPO, SUM(AF4_QUANT*AF2_QUANT*AF4_VLRUNI) TOTAL "
		cQryMA+="FROM " + RetSqlName("AF4") + " AF4 "
		cQryMA+="INNER JOIN " + RetSqlName("AF2") + " AF2 "
		cQryMA+="ON AF4_FILIAL = AF2_FILIAL "
		cQryMA+="AND AF4_ORCAME = AF2_ORCAME "
		cQryMA+="AND AF4_TAREFA = AF2_TAREFA "
		cQryMA+="WHERE AF4.D_E_L_E_T_ <> '*' "
		cQryMA+="AND AF2.D_E_L_E_T_ <> '*' "
		cQryMA+="AND AF4_ORCAME = '"	+ (cAliasTop)->AF1_ORCAME	+ "' " 
		cQryMA+="AND AF4_FILIAL =  '"	+ xFilial("AF4")				+ "' "
		cQryMA+="GROUP BY AF4_TIPOD "
		cQryMA+="ORDER BY TOTAL DESC "
		
		dbUseArea(.T.,"TOPCONN",TCGenQry(,,cQryMA),"XMA",.F.,.T.)
		
		XMA->(dbGoTop())
		
		While XMA->(!EOF())
		    
		    If nLin >= nMaxLin
				CNRCabec(Nil,Nil,OemToAnsi(cTitRel),.t.) //-- Impressao do Cabecalho da pagina
			EndIf
			
			Do Case
			Case XMA->TIPO == "MA"
				cCod   :=XMA->COD
				cDescri:=AllTrim(Posicione("SB1",1,xFilial("SB1")+XMA->COD,"B1_CEME"))
				cUM    :=Posicione("SB1",1,xFilial("SB1")+XMA->COD,"B1_UM")
				nTotPct+=XMA->TOTAL/nTotal*100
			Case XMA->TIPO == "MO"
				cCod   :=XMA->COD
				cDescri:=Posicione("AE8",1,xFilial("AE8")+XMA->COD,"AE8_DESCRI")
				cUM    :="H"
				nTotPct+=XMA->TOTAL/nTotal*100
			Case XMA->TIPO == "DP"
			    cCod   :=XMA->COD
				cDescri:=Posicione("SZ3",1,xFilial("SZ3")+XMA->COD,"Z3_DESCRI")
				cUM    :=Posicione("SZ3",1,xFilial("SZ3")+XMA->COD,"Z3_UM")
				nTotPct+=(XMA->QUANT*XMA->VLRUNI)*100/nTotal
			EndCase
			
			If Len(cDescri) < 51
				CNRBox(0050,0335,cCod 												,	,.F.,2,oFont08,.F.)
				CNRBox(0335,1205,cDescri												,	,.F.,2,oFont08,.F.)
				CNRBox(1205,1295,cUM 												, 	,.F.,2,oFont08,.F.)
				CNRBox(1295,1580,Transform(XMA->VLRUNI,"@E 999,999,999.99") 	,""	,.F.,2,oFont08,.T.)
				CNRBox(1580,1865,Transform(XMA->QUANT,"@E 999,999,999.9999") 	,""	,.F.,2,oFont08,.T.)
				CNRBox(1865,2150,Transform(XMA->TOTAL,"@E 999,999,999.99") 		,""	,.F.,2,oFont08,.T.)
				CNRBox(2150,2450,Transform(XMA->TOTAL/nTotal*100,"@E 999.999%")+" "+Transform(nTotPct,"@E 999.999%"),""    ,.T.,2,oFont07n,.F.)
			Else
				If Len(cDescri) < 101
					CNRBox(0050,0335,cCod					, 													,.F.,3,oFont08,.F.)
					CNRBox(0335,1205,Substr(cDescri,1,50)	,Substr(cDescri,51,100)							,.F.,3,oFont08,.F.)
					CNRBox(1205,1295,cUM						, 													,.F.,3,oFont08,.F.)
					CNRBox(1295,1580,""						,Transform(XMA->VLRUNI,"@E 999,999,999.99") 	,.F.,3,oFont08,.T.)
					CNRBox(1580,1865,""						,Transform(XMA->QUANT,"@E 999,999,999.9999") 	,.F.,3,oFont08,.T.)
					CNRBox(1865,2150,""						,Transform(XMA->TOTAL,"@E 999,999,999.99") 	,.F.,3,oFont08,.T.)
					CNRBox(2150,2450,""						,Transform(XMA->TOTAL/nTotal*100,"@E 999.999%")+" "+Transform(nTotPct,"@E 999.999%")    ,.T.,3,oFont07n,.F.)
				Else
					If Len(cDescri) < 151
						CNRBox(0050,0335,cCod					, 													,.F.,5,oFont08,.F.)
						CNRBox(0335,1205,Substr(cDescri,1,50)	,Substr(cDescri,51,150)							,.F.,5,oFont08,.F.)
						CNRBox(1205,1295,cUM						, 													,.F.,5,oFont08,.F.)
						CNRBox(1295,1580,""						,Transform(XMA->VLRUNI,"@E 999,999,999.99") 	,.F.,5,oFont08,.T.)
						CNRBox(1580,1865,""						,Transform(XMA->QUANT,"@E 999,999,999.9999") 	,.F.,5,oFont08,.T.)
						CNRBox(1865,2150,""						,Transform(XMA->TOTAL,"@E 999,999,999.99") 	,.F.,5,oFont08,.T.)
						CNRBox(2150,2450,""						,Transform(XMA->TOTAL/nTotal*100,"@E 999.999%")+" "+Transform(nTotPct,"@E 999.999%")    ,.T.,5,oFont07n,.F.)
					Else
						CNRBox(0050,0335,cCod					, 													,.F.,6,oFont08,.F.)
						CNRBox(0335,1205,Substr(cDescri,1,50)	,Substr(cDescri,51,200)							,.F.,6,oFont08,.F.)
						CNRBox(1205,1295,cUM						, 													,.F.,6,oFont08,.F.)
						CNRBox(1295,1580,""						,Transform(XMA->VLRUNI,"@E 999,999,999.99") 	,.F.,6,oFont08,.T.)
						CNRBox(1580,1865,""						,Transform(XMA->QUANT,"@E 999,999,999.9999") 	,.F.,6,oFont08,.T.)
						CNRBox(1865,2150,""						,Transform(XMA->TOTAL,"@E 999,999,999.99") 	,.F.,6,oFont08,.T.)
						CNRBox(2150,2450,""						,Transform(XMA->TOTAL/nTotal*100,"@E 999.999%")+" "+Transform(nTotPct,"@E 999.999%")    ,.T.,6,oFont07n,.F.)
					EndIf
				EndIf
			EndIf
				
			XMA->(dbSkip())
		EndDo
		
		dbSelectArea("XMA")
		dbCloseArea()
		
		CNRBox(0335,1205,"TOTAL GERAL"                        ,               	,.F.,4,oFont08n,.F.)
		CNRBox(1865,2150,Transform(nTotal,"@E 999,999,999.99"),""     				,.T.,4,oFont08n,.T.)		
	EndIf
			
	nLin := 3000
		
	(cAliasTop)->(dbSkip())
EndDo

