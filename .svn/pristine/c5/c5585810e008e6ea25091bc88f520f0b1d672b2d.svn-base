	cQuery  += ".And.DtoS(C2_EMISSAO)>='"+DtoS(mv_par07)+"'.And.DtoS(C2_EMISSAO)<='"+DtoS(mv_par08)+"'"
	dbSelectArea("SC2")
	dbSetOrder(1)
	dbSetFilter({|| &cQuery},cQuery)
	cAliasSC2 := "SC2"

