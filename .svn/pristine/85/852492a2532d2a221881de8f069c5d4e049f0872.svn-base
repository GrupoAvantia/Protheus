Function U_MatrBol3()
Local cPerg		:= "MTRBO3"
Local cTitulo	:= "Livro de" + " " + "Compras" + "/" + "Vendas"
Local cDescr1	:= If( cPaisLoc $ "ANG|PTG", "Impressão do livro fiscal (compras/vendas)", "Impressao do livro fiscal (compras/vendas)" )
Local cNomeProg	:= "MATRBOL3"
Local aArea		:= {}

