Static Function BOL3Impr(lEnd)
Local nLin		:= 0
Local nPag		:= 0
Local nAltPag	:= 0
Local nLarPag	:= 0
Local nCpo		:= 0
Local nPos		:= 0
Local nEspTot	:= 0
Local cTitulo	:= ""
Local cTit		:= ""
Local aTitulo	:= {}
Local aDados	:= {}

Private aCposImp	:= {}
Private aTitDet		:= {}
Private aCab		:= {}
Private cLinDet		:= ""
Private nLinTit		:= 0
Private nTotFat		:= 0
Private nTotIC		:= 0
Private nTotIse		:= 0
Private nTotBas		:= 0
Private nTotIVA		:= 0






If MV_PAR03 == 1
	MsgRun(If( cPaisLoc $ "ANG|PTG", "A preparar as informações para o livro de compras", "Preparando as informacoes para o livro de compras" ),"Livros fiscais",{|| DaVinci("C")})


	Aadd(aCposImp,{"TIPONF",07,"","C",If( cPaisLoc $ "ANG|PTG", "Tipo de factura", "Tipo de fatura" )})
	Aadd(aCposImp,{"NIT",10,"","C","NIT do fornecedor"})
	Aadd(aCposImp,{"RAZSOC",25,"","C",If( cPaisLoc $ "ANG|PTG", "Razão social", "Razao social" )})
	Aadd(aCposImp,{"NFISCAL",12,"","C",If( cPaisLoc $ "ANG|PTG", "Factura", "Fatura" )})
	Aadd(aCposImp,{"POLIZA",20,"","C",If( cPaisLoc $ "ANG|PTG", "Poliza de importação", "Poliza de importacao" )})
	Aadd(aCposImp,{"NUMAUT",15,"","C",If( cPaisLoc $ "ANG|PTG", "Número de autorização", "Numero de autorizacao" )})
	Aadd(aCposImp,{"EMISSAO",10,"","D",If( cPaisLoc $ "ANG|PTG", "Data da factura", "Data da fatura" )})
	Aadd(aCposImp,{"VALCONT",17,"@E 99,999,999,999.99","N",If( cPaisLoc $ "ANG|PTG", "Valor total da factura", "Valor total da fatura" )})
	Aadd(aCposImp,{"ICE",17,"@E 99,999,999,999.99","N","Valor ICE"})
	Aadd(aCposImp,{"EXENTAS",17,"@E 99,999,999,999.99","N","Valores isentos ou a taxa zero"})
	Aadd(aCposImp,{"BASEIMP",17,"@E 99,999,999,999.99","N",If( cPaisLoc $ "ANG|PTG", "Valor sujeito a crédito fiscal", "Valor sujeito a credito fiscal" )})
	Aadd(aCposImp,{"VALIMP",17,"@E 99,999,999,999.99","N",If( cPaisLoc $ "ANG|PTG", "Crédito fiscal", "Credito fiscal" )})
	Aadd(aCposImp,{"CODCTR",14,"","C",If( cPaisLoc $ "ANG|PTG", "Código de controlo", "Codigo de controle" )})
Else
	MsgRun(If( cPaisLoc $ "ANG|PTG", "A preparar as informações para o livro de vendas", "Preparando as informacoes para o livro de vendas" ),"Livros fiscais",{|| DaVinci("V")})


	Aadd(aCposImp,{"NIT",10,"","C","NIT"})
	Aadd(aCposImp,{"RAZSOC",25,"","C",If( cPaisLoc $ "ANG|PTG", "Razão social", "Razao social" )})
	Aadd(aCposImp,{"NFISCAL",12,"","C",If( cPaisLoc $ "ANG|PTG", "Factura", "Fatura" )})
	Aadd(aCposImp,{"NUMAUT",15,"","C",If( cPaisLoc $ "ANG|PTG", "Número de autorização", "Numero de autorizacao" )})
	Aadd(aCposImp,{"EMISSAO",10,"","D",If( cPaisLoc $ "ANG|PTG", "Data da factura", "Data da fatura" )})
	Aadd(aCposImp,{"VALCONT",17,"@E 99,999,999,999.99","N",If( cPaisLoc $ "ANG|PTG", "Valor total da factura", "Valor total da fatura" )})
	Aadd(aCposImp,{"ICE",17,"@E 99,999,999,999.99","N","Valor ICE"})
	Aadd(aCposImp,{"EXENTAS",17,"@E 99,999,999,999.99","N","Valores isentos ou a taxa zero"})
	Aadd(aCposImp,{"BASEIMP",17,"@E 99,999,999,999.99","N",If( cPaisLoc $ "ANG|PTG", "Valor sujeito a débito fiscal", "Valor sujeito a debito fiscal" )})
	Aadd(aCposImp,{"VALIMP",17,"@E 99,999,999,999.99","N",If( cPaisLoc $ "ANG|PTG", "Débito fiscal", "Debito fiscal" )})
	Aadd(aCposImp,{"STATUSNF",9,"","C",If( cPaisLoc $ "ANG|PTG", "Estado da factura", "Estado da fatura" )})
	Aadd(aCposImp,{"CODCTR",14,"","C",If( cPaisLoc $ "ANG|PTG", "Código de controlo", "Codigo de controle" )})
Endif


For nCpo := 1 To Len(aCposImp)
	If nCpo > 1
		cLinDet += " "
	Endif
	cLinDet += Replicate("#",aCposImp[nCpo,2])
Next



(cAliasLCV)->(DbGoTop())
nLarPag := Len(cLinDet)
nAltPag := 62
nLinTit := 0
aTitDet := {}
For nCpo := 1 To Len(aCposImp)
	cTitulo := Upper(AllTrim(aCposImp[nCpo,5]))
	aTitulo := {}
	While !Empty(cTitulo)
		cTit := AllTrim(Substr(cTitulo,1,aCposImp[nCpo,2]))
		nPos := aCposImp[nCpo,2]
		If !Empty(Substr(cTitulo,aCposImp[nCpo,2] + 1,1))
			While nPos > 0 .And.  !Empty(Substr(cTitulo,nPos,1))
				nPos--
			Enddo
			If nPos > 0
				cTit := Substr(cTitulo,1,nPos - 1)
			Else
				nPos := aCposImp[nCpo,2]
			Endif
		Endif

		If aCposImp[nCpo,4] == "N"
			cTit := PadL(cTit,aCposImp[nCpo,2])
		Endif
		Aadd(aTitulo,cTit)
		cTitulo := LTrim(Substr(cTitulo,nPos + 1))
	Enddo
	nLinTit := Max(nLinTit,Len(aTitulo))
	Aadd(aTitDet,aTitulo)
Next

nTotFat	:= 0
nTotIC	:= 0
nTotIse	:= 0
nTotBas	:= 0
nTotIVA := 0
SetRegua((cAliasLCV)->(RecCount()))
While !((cAliasLCV)->(Eof())) .And.  !lEnd


	nPag++
	nLin := BOL3Cabec(nPag)


	nPos := 0
	While !((cAliasLCV)->(Eof())) .And.  nLin < nAltPag .And.  !lEnd
		aDados := {}
		For nCpo := 1 To Len(aCposImp)
			If Empty(aCposImp[nCpo,3])
				Aadd(aDados,PadR((cAliasLCV)->(FieldGet(FieldPos(aCposImp[nCpo,1]))),aCposImp[nCpo,2]))
			Else
				Aadd(aDados,Transform((cAliasLCV)->(FieldGet(FieldPos(aCposImp[nCpo,1]))),aCposImp[nCpo,3]))
			Endif
		Next
		FmtLin(aDados,cLinDet,,,@nLin)
		If AllTrim((cAliasLCV)->STATUSNF) == "V"
			nTotFat += (cAliasLCV)->VALCONT
			nTotIC  += (cAliasLCV)->ICE
			nTotIse += (cAliasLCV)->EXENTAS
			nTotBas += (cAliasLCV)->BASEIMP
			nTotIVA += (cAliasLCV)->VALIMP
		Endif
		If lEnd
			(cAliasLCV)->(DbGoBottom())
		Endif
		(cAliasLCV)->(DbSkip())
		IncRegua()
	Enddo
	If (cAliasLCV)->(Eof()) .And.  !lEnd
		If nTotFat <> 0
	  		BOL3Totais()
		Endif
	Endif
Enddo
If Select(cAliasLCV) > 0
	DbSelectArea(cAliasLCV)
	DbCloseArea()
Endif
If lEnd
	cTit := Upper(AllTrim(If( cPaisLoc $ "ANG|PTG", "Impressão cancelada pelo utilizador", "Impressao cancelada pelo usuario" )))
	nPos := (nLarPag - Len(cTit)) / 2
	PrintOut(_PROW()+2,nPos,cTit,,)
Endif

If aReturn[5]==1
   dbCommitAll()
   Set( 24, "" )
   OurSpool(wnRel)
Endif
MS_FLUSH()
Return()















Static Function BOL3Cabec(nPag)
