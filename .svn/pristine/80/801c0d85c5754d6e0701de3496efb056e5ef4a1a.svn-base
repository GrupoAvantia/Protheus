	Case nOrdem == 2
		cKeyForTot := "D3_OP"
		cSubTotal  := If( cPaisLoc $ "ANG|PTG", "......... total da ordem de produção ", "......... Total da Ordem de Producao " )
	Case nOrdem == 3
		cKeyForTot := "D3_CF"
		cSubTotal  := If( cPaisLoc $ "ANG|PTG", "......... total da classificação ", "......... Total da Classificacao " )
	Case nOrdem == 4
		cKeyForTot := "D3_TIPO"
		cSubTotal  := If( cPaisLoc $ "ANG|PTG", "......... total do tipo ", "......... Total do Tipo " )
	Case nOrdem == 5
		cKeyForTot := "D3_GRUPO"
		cSubTotal  := If( cPaisLoc $ "ANG|PTG", "......... total do grupo ", "......... Total do Grupo " )
	Case nOrdem == 6
		cKeyForTot := "D3_CC"
		cSubTotal  := If( cPaisLoc $ "ANG|PTG", "......... total do centro de custo ", "......... Total do Centro de Custo " )
	Case nOrdem == 7
		cKeyForTot := "D3_CONTA"
		cSubTotal  := If( cPaisLoc $ "ANG|PTG", "......... total da conta ", "......... Total da Conta " )
	Case nOrdem == 8
		cKeyForTot := "D3_DOC"
		cSubTotal  := If( cPaisLoc $ "ANG|PTG", "......... total do documento ", "......... Total do Documento " )
EndCase




