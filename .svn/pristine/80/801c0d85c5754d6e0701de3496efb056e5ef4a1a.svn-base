oReport:SetMeter(SD3->(LastRec()))
dbSelectArea(cAliasSD3)

While !oReport:Cancel() .And.  !(cAliasSD3)->(Eof())

	oReport:IncMeter()
	If oReport:Cancel()
		Exit
	EndIf

	If UPPER((cAliasSD3)->D3_DOC) < UPPER(mv_par21) .Or.  UPPER((cAliasSD3)->D3_DOC) > UPPER(mv_par22)
		(cAliasSD3)->(dbSkip())
		Loop
	EndIf
	If !D3Valido(cAliasSD3)
		(cAliasSD3)->(dbSkip())
		Loop
	EndIf

 	oSection1:Init()
 	oSection1:PrintLine()

	nProdSegEnt := 0
	nProdSegSai := 0

	cChave := (cAliasSD3)->&(cKeyForTot)
	cCodPro:= (cAliasSD3)->D3_COD

 	oSection2:Init()
	While !oReport:Cancel() .And.  !(cAliasSD3)->(Eof()) .And.  cChave == (cAliasSD3)->&(cKeyForTot) .And.  cCodPro == (cAliasSD3)->D3_COD

		oReport:IncMeter()
		If oReport:Cancel()
			Exit
		EndIf

		If UPPER((cAliasSD3)->D3_DOC) < UPPER(mv_par21) .Or.  UPPER((cAliasSD3)->D3_DOC) > UPPER(mv_par22)
			(cAliasSD3)->(dbSkip())
			Loop
		EndIf

		If !D3Valido(cAliasSD3)
			(cAliasSD3)->(dbSkip())
			Loop
		EndIf

  		cTotaliza := (cAliasSD3)->&(cKeyForTot)

   		oSection2:PrintLine()


		If (cAliasSD3)->D3_TM <= "500"
			nProdSegEnt += If(SB1->B1_CONV>0,ConvUm((cAliasSD3)->D3_COD,(cAliasSD3)->D3_QUANT,0,2),(cAliasSD3)->D3_QTSEGUM )
        Else
			nProdSegSai += If(SB1->B1_CONV>0,ConvUm((cAliasSD3)->D3_COD,(cAliasSD3)->D3_QUANT,0,2),(cAliasSD3)->D3_QTSEGUM )
        EndIf

		cSubCtb := ""
		If nOrdem == 6 .Or.  nOrdem == 7

			If nOrdem == 6 .And.  (aFileCtb[2])->(dbSeek(xFilial(aFileCtb[2])+(cAliasSD3)->D3_CC, .F. )) .Or.  nOrdem == 7 .And.  (aFileCtb[1])->(dbSeek(xFilial(aFileCtb[1])+(cAliasSD3)->D3_CONTA, .F. ))
			   cSubCtb := &(cCposCtb)
			ElseIf !Empty((cAliasSD3)->D3_CC) .Or.  !Empty((cAliasSD3)->D3_CONTA)
			   cSubCtb := If( cPaisLoc $ "ANG|PTG", " ** cc não registado ** ", " ** CC nao cadastrado ** " )
			EndIf
		EndIf

		dbSelectArea(cAliasSD3)
		dbSkip()

	EndDo


	oReport:PrintText(If( cPaisLoc $ "ANG|PTG", "Quantidade na seg. um (", "Quantidade na Seg. UM (" )+SB1->B1_SEGUM+ If( cPaisLoc $ "ANG|PTG", "): entrada -> ", "): Entrada -> " )+TransForm(nProdSegEnt,cPicD3Cust)+"   "+ If( cPaisLoc $ "ANG|PTG", "Saída -> ", "Saida -> " )+TransForm(nProdSegSai,cPicD3Cust),,oSection1:Cell("B1_UM"):ColPos())


	oSection1:Finish()
	nGSegEnt += nProdSegEnt
	nGSegSai += nProdSegSai
	nTotSegEnt += nProdSegEnt
	nTotSegSai += nProdSegSai

	If cTotaliza <> (cAliasSD3)->&(cKeyForTot) .Or.  (cAliasSD3)->(Eof())

		oSection2 :SetTotalText( cSubTotal + cTotaliza + cSubCtb )

		If (cAliasSD3)->(Eof())
			If nOrdem ==1
				nProdSegEnt:=nGSegEnt
				nProdSegSai:=nGSegSai
			Else
				nTotSegEnt:=nGSegEnt
				nTotSegSai:=nGSegSai
			EndIf
		EndIf
  		oSection2:Finish()
		If (cAliasSD3)->(Eof())

			oReport:PrintText(PadR(If( cPaisLoc $ "ANG|PTG", "Total Geral Qtde.2a.um:", "Total Geral Qtde.2a.UM:" ),Len(If( cPaisLoc $ "ANG|PTG", "Quantidade na seg. um (", "Quantidade na Seg. UM (" ))+2+TamSx3("B1_SEGUM")[1])+Substr(If( cPaisLoc $ "ANG|PTG", "): entrada -> ", "): Entrada -> " ),3)+TransForm(nGSegEnt,cPicD3Cust)+"   "+ If( cPaisLoc $ "ANG|PTG", "Saída -> ", "Saida -> " )+TransForm(nGSegSai,cPicD3Cust),oReport:Row()+2,oSection1:Cell("B1_UM"):ColPos())

		ElseIf nOrdem <> 1 .And.  cTotaliza <> (cAliasSD3)->&(cKeyForTot)

			oReport:PrintText(Left(If( cPaisLoc $ "ANG|PTG", "Quantidade na seg. um (", "Quantidade na Seg. UM (" ),Len(If( cPaisLoc $ "ANG|PTG", "Quantidade na seg. um (", "Quantidade na Seg. UM (" ))-2)+Space(2+TamSx3("B1_SEGUM")[1])+"  "+Substr(If( cPaisLoc $ "ANG|PTG", "): entrada -> ", "): Entrada -> " ),3)+TransForm(nTotSegEnt,cPicD3Cust)+"   "+ If( cPaisLoc $ "ANG|PTG", "Saída -> ", "Saida -> " )+TransForm(nTotSegSai,cPicD3Cust),,oSection1:Cell("B1_UM"):ColPos())
			nTotSegEnt:=0
			nTotSegSai:=0
		EndIf

  	EndIf

EndDo

Return Nil






























Function MATR300R3



