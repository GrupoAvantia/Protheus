Static Function CopyOrc()

cOrcMod:=mv_par01
If mv_par04 == 1
   cTamOrc:=str(len(Alltrim(cOrcMod)))
   cQuery:="SELECT COUNT(R_E_C_N_O_) AS TOTREG "
   cQuery+="FROM "+RetSqlName("AF1")+" "
   cQuery+="WHERE D_E_L_E_T_ <> '*' "
   cQuery+="AND LEFT(RTRIM(LTRIM(AF1_ORCAME)),7) = '"+Substr(cOrcMod,1,7)+"' "
   
   TCQUERY cQuery NEW ALIAS "XAF1"
   
   dbSelectArea("XAF1")
            
   nRevOrc := XAF1->TOTREG+1-1
   
   dbSelectArea("XAF1")
   dbCloseArea()
   
   cOrcNew := Substr(cOrcMod,1,7)+"."+strzero(nRevOrc,2)
Else
   cOrcNew:=CodOrc()   
EndIf	

If Empty(cOrcMod)
	Alert("Informe o Oramento de origem","Ateno")
	
	Return
EndIf

If Empty(cOrcNew)
	Alert("Informe o Oramento destino","Ateno")
	
	Return
EndIf

dbSelectArea("AF1")
dbSetOrder(1)
If !AF1->(dbSeek(xFilial("AF1")+mv_par01))
	Alert("Orcamento de origem nao existe","Ateno")
	
	Return
EndIf


