//Funo para grao do codigo do projeto
Static Function CodOrc()

cCod := space(10)

dbSelectArea("SX6")
dbSetOrder(1)

If !SX6->(dbSeek("  "+Padr("MV_SEQPRJ",10)))
	RecLock("SX6",.T.)
	SX6->X6_FIL      := "  "
	SX6->X6_VAR   	 := "MV_SEQPRJ"
	SX6->X6_TIPO  	 := "C"
	SX6->X6_DESCRIC  := SX6->X6_DSCSPA  := 	SX6->X6_DSCENG  := "Sequencia do Projeto/Orcamento"
	SX6->X6_CONTEUD  := SX6->X6_CONTSPA :=  SX6->X6_CONTENG := "001"
	SX6->X6_PROPRI   := 'U'
	SX6->(MsUnlock())

    cCod := Alltrim(SX6->X6_CONTEUD)+Substr(dtos(dDatabase),5,2)+Substr(dtos(dDatabase),3,2)+".00"
Else
	cSeq := GetMv("MV_SEQPRJ")
	
	cCod := Alltrim(cSeq)+Substr(dtos(dDatabase),5,2)+Substr(dtos(dDatabase),3,2)+".00"
EndIf

Return cCod



//Funo para grao do codigo do projeto
Static Function CodNewOrc()

dbSelectArea("SX6")
dbSetOrder(1)

cSeq := GetMv("MV_SEQPRJ")

If SX6->(dbSeek("  "+Padr("MV_SEQPRJ",10)))
	Reclock("SX6",.f.)
	Replace SX6->X6_CONTEUD With StrZero(val(cSeq)+1,3)
	Replace SX6->X6_CONTSPA With StrZero(val(cSeq)+1,3)
	Replace SX6->X6_CONTENG With StrZero(val(cSeq)+1,3)
	MsUnlock()		
EndIf

Return