		    If Alltrim(cCampo) == "AJF_ORCAME" .Or. Alltrim(cCampo) == "AJF_EDT"
		    	Replace AJF->&(cCampo) With cOrcNew
		    Else
		    	Replace AJF->&(cCampo) With XAJF->&(cCampo)
		    EndIf	
				
			SX3->(dbSkip())
		EndDo		
		MsUnlock()
	EndIf	

	XAJF->(dbSkip())
EndDo

dbSelectArea("XAJF")
dbCloseArea()

